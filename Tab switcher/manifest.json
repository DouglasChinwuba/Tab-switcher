{
  "manifest_version": 3,
  "name": "Tab Switcher",
  "version": "1.0",
  "action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "description": "Easily switch between chrome tabs",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "permissions": ["tabs"]
}
